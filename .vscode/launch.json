{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "ts-node",
        "type": "node",
        "request": "launch",
        "runtimeExecutable": "node",
        "runtimeArgs": ["--nolazy","--trace-warnings", "-r", "ts-node/register/transpile-only"],
  
        "args": ["app.ts"],
        
        "cwd": "${workspaceRoot}",
        "internalConsoleOptions": "openOnSessionStart",
      }
    ]
  }